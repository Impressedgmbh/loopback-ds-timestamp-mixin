    "debug": "^2.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "coveralls": "^2.11.16",
    "eslint": "^3.16.1",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.0.1",
    "loopback": "=3.4.0",
    "loopback-boot": "=2.23.0",
    "loopback-datasource-juggler": "=3.1.1",
